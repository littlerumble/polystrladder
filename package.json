{
    "name": "sharkbot-copy",
    "version": "1.0.0",
    "description": "Polymarket copy trading bot with paper trading simulation",
    "main": "dist/index.js",
    "scripts": {
        "build": "npm run build:backend && npm run build:dashboard",
        "build:backend": "tsc",
        "build:dashboard": "cd dashboard && npm install && npm run build",
        "start": "npx prisma db push && node dist/index.js",
        "dev": "ts-node src/index.ts",
        "dev:dashboard": "cd dashboard && npm run dev",
        "prisma:generate": "prisma generate",
        "prisma:push": "prisma db push",
        "prisma:migrate": "prisma migrate dev",
        "test": "ts-node tests/runTests.ts",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@prisma/client": "^5.7.0",
        "axios": "^1.6.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.4",
        "@types/ws": "^8.5.10",
        "prisma": "^5.7.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
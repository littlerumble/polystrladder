# Example environment variables for Railway
# Copy these to Railway Dashboard > Variables

# MySQL Database (Railway will provide this)
DATABASE_URL="mysql://user:pass@host:port/database"

# Whale to track
WHALE_ADDRESS="0x2005d16a84ceefa912d4e380cd32e7ff827875ea"

# API URLs (Polymarket public APIs)
CLOB_API_URL="https://clob.polymarket.com"
GAMMA_API_URL="https://gamma-api.polymarket.com"
DATA_API_URL="https://data-api.polymarket.com"

# Server port (Railway sets this automatically via $PORT)
PORT=3002
